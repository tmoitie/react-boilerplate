{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "For quick react projects",
  "main": "index.js",
  "engines": {
    "node": "6.5.0",
    "npm": "3.10.6"
  },
  "scripts": {
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 _mocha --compilers js:babel-register --require ./test/setup.js 'test/**/*.spec.js'",
    "test-watch": "npm test; chokidar 'src/**/*.js' 'test/**/*.js' -c 'npm test'",
    "coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.tmoitie.uk:tmoitie/react-boilerplate.git"
  },
  "author": "Tom Moiti√©",
  "license": "UNLICENSED",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "immutable-devtools": "0.0.7",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.1",
    "postcss-loader": "^1.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "next",
    "react-immutable-proptypes": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^3.1.3",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.2.0",
    "isparta-loader": "^2.0.0",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
